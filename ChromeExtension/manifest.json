{
    "manifest_version": 3,
    "name": "Gen Chrome Extension",
    "description": "FamilySearch Scraping Chrome Extension",
    "version": "0.1.0",
    "homepage_url": "https://github.com/WilliamSampaio/gen",
    "action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs",
        "webNavigation"
    ],
    "host_permissions": [
        "*://*/*"
    ]
}