{
    "manifest_version": 3,
    "name": "WebScrapingChromeExtension",
    "description": "Web scraping Chrome extension",
    "version": "0.1.0",
    "homepage_url": "https://github.com/WilliamSampaio/gen",
    "action": {
        "default_icon": "icon.png",
        "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],
    "host_permissions": ["*://*/*"]
}